{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Launch src/__main__.py (Poetry venv, AP_DEV_MODE=1)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/__main__.py",
            "console": "integratedTerminal",
            "env": {
                "AP_DEV_MODE": "1"
            },
            "python": "${workspaceFolder}/.venv/bin/python",
            "cwd": "${workspaceFolder}"
        }
    ]
}